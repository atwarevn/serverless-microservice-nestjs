{
  "name": "serverless-microservice",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "npx serverless dynamodb install",
    "format": "nx format:write",
    "start": "sh -c 'nx run ${0}:build:${1} --watch & npx serverless offline start'",
    "build": "sh -c 'nx build ${0}'",
    "build:layerModules": "yarn install --production --modules-folder ./dist/node_modules",
    "deploy": "sh -c 'yarn local:installLayerModule && nx build ${0} && cdk deploy'"
  },
  "private": true,
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.58.0",
    "@aws-sdk/lib-dynamodb": "^3.58.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@vendia/serverless-express": "^4.5.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "source-map-support": "^0.5.16",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.3.2",
    "@nrwl/eslint-plugin-nx": "13.3.2",
    "@nrwl/jest": "13.3.2",
    "@nrwl/linter": "13.3.2",
    "@nrwl/nest": "13.3.2",
    "@nrwl/node": "13.3.2",
    "@nrwl/tao": "13.3.2",
    "@nrwl/workspace": "13.3.2",
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "aws-cdk": "2.8.0",
    "aws-cdk-lib": "2.8.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "prettier": "^2.3.1",
    "serverless": "^3.10.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^8.5.0",
    "ts-jest": "27.0.5",
    "ts-node": "^9.0.0",
    "typescript": "~4.4.3"
  }
}
